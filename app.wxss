/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.divider{
    height:20rpx;
    background: #f1f1f1;
    box-shadow: 0 5rpx 5rpx rgba(0,0,0,.1) inset;
}
.ell{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}